    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
