{
  "name": "hui-grid",
  "private": true,
  "version": "1.0.0",
  "description": "Vanilla javascript data grid",
  "type": "module",
  "main": "dist/hui-grid.js",
  "module": "dist/es/hui-grid.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "grid",
    "datagrid",
    "vanilla",
    "hui"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d0orHyeok/hui.grid.git"
  },
  "exports": {
    ".": {
      "require": "./dist/hui-grid.js",
      "import": "./dist/es/hui-grid.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "clean": "node scripts/clean.js",
    "build": "(npm run clean) && (npm run build:rollup) && (node scripts/erase.js)",
    "build:rollup": "rollup --config",
    "build-publish": "(npm run build) && (node scripts/setupPackage.js) && cd dist && npm publish",
    "lint:scripts": "eslint . --ext .ts,.js",
    "lint:styles": "stylelint ./**/*.{css,scss}",
    "format:scripts": "prettier . --write",
    "format:styles": "stylelint ./**/*.{css,scss} --fix",
    "format": "npm run format:scripts && npm run format:styles"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": "eslint --fix",
    "*src/**/*.css.": "prettier --write"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-flow": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/runtime": "^7.23.2",
    "@rollup/plugin-alias": "^5.0.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^20.4.9",
    "@types/to-px": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.14",
    "babel": "^6.23.0",
    "cssnano": "^6.0.1",
    "csstype": "^3.1.2",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "postcss-custom-properties": "^13.3.2",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^9.3.0",
    "prettier": "3.0.0",
    "rollup": "^3.29.4",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.35.0",
    "stylelint": "^15.10.2",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-prettier": "^4.0.2",
    "terser": "^5.19.2",
    "tslib": "^2.6.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.0"
  },
  "dependencies": {
    "dayjs": "^1.11.9",
    "lodash-es": "^4.17.21",
    "to-px": "^1.1.0"
  }
}
